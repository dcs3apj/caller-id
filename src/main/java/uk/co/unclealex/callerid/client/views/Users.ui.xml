  xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:a="urn:import:uk.co.unclealex.callerid.client.ui">

	<g:HTMLPanel>
		<a:UnorderedListPanel ui:field="existingUsersPanel"/>
